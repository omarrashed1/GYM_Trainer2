<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F2F3FA"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="104dp"
        android:layout_height="23dp"
        android:text="حدد التمرين"
        android:textColor="@color/black"
        android:textSize="19dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.947"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.473" />

    <ScrollView
        android:id="@+id/horizontalScrollView1"
        android:layout_width="306dp"
        android:layout_height="330dp"
        android:layout_margin="8dp"

        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.93">


        <GridLayout
            android:id="@+id/gridLayout"
            android:layout_width="351dp"
            android:layout_height="281dp"
            android:columnCount="2"
            android:rowCount="2"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.733"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.964"
            tools:ignore="MissingConstraints">


            <ImageView
                android:id="@+id/imagebutton2"
                android:layout_width="106dp"
                android:layout_height="90dp"
                android:layout_row="0"
                android:layout_column="0"

                android:layout_margin="25dp"
                android:background="@drawable/selector"
                app:srcCompat="@drawable/arm22"
                tools:layout_editor_absoluteX="4dp"
                tools:layout_editor_absoluteY="139dp" />

            <ImageView
                android:id="@+id/imagebutton4"
                android:layout_width="106dp"
                android:layout_height="90dp"
                android:layout_margin="25dp"
                app:srcCompat="@drawable/arm22"
                tools:layout_editor_absoluteX="152dp"
                tools:layout_editor_absoluteY="139dp" />

            <ImageView
                android:id="@+id/imagebutton5"
                android:layout_width="106dp"
                android:layout_height="90dp"
                android:layout_margin="25dp"
                app:srcCompat="@drawable/arm22"
                tools:layout_editor_absoluteX="152dp"
                tools:layout_editor_absoluteY="139dp" />

            <ImageView
                android:id="@+id/imagebutton6"
                android:layout_width="106dp"
                android:layout_height="90dp"
                android:layout_margin="25dp"
                app:srcCompat="@drawable/arm22"
                tools:layout_editor_absoluteX="152dp"
                tools:layout_editor_absoluteY="139dp" />

            <ImageView
                android:id="@+id/imagebutton3"
                android:layout_width="106dp"
                android:layout_height="90dp"
                android:layout_margin="25dp"
                app:srcCompat="@drawable/arm22"
                tools:layout_editor_absoluteX="152dp"
                tools:layout_editor_absoluteY="139dp" />


        </GridLayout>
    </ScrollView>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="343dp"
        android:layout_height="194dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.029"
        app:srcCompat="@drawable/rectangle1" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="173dp"
        android:layout_height="58dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/rectangle2" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="349dp"
        android:layout_height="103dp"

        android:layout_marginTop="228dp"
        app:layout_constraintBottom_toBottomOf="@+id/horizontalScrollView1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.539"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/rectangle3" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="81dp"
        android:layout_height="81dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:srcCompat="@drawable/rrunning" />


</androidx.constraintlayout.widget.ConstraintLayout>